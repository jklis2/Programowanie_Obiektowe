<dx:ThemedWindow
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    xmlns:dxr="http://schemas.devexpress.com/winfx/2008/xaml/ribbon" 
    xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars"
    x:Class="DXApplication1.MainWindow"
    Title="MainWindow" Height="800" Width="1552">
    <Grid>
        <dxr:RibbonControl HorizontalAlignment="Stretch" RibbonStyle="Office2019" VerticalAlignment="Top">
            <dxr:RibbonDefaultPageCategory>
                <dxr:RibbonPage Caption="Pracownicy">
                    <dxr:RibbonPageGroup Caption="Cos">
                        <dxb:BarButtonItem x:Name="Dodaj" Content="Dodaj pracownika" Glyph="{dx:DXImage SvgImages/Business Objects/BO_Position_v92.svg}" RibbonStyle="Large" ItemClick="DodajPracownika">
                            
                        </dxb:BarButtonItem>
                    </dxr:RibbonPageGroup>
                </dxr:RibbonPage>
            </dxr:RibbonDefaultPageCategory>
            
        </dxr:RibbonControl>

    </Grid>
</dx:ThemedWindow>
